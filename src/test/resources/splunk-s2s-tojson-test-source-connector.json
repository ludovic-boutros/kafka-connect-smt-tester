{
  "name": "SplunkS2SSourceConnector_9997",
  "config": {
    "connector.class": "io.confluent.connect.splunk.s2s.SplunkS2SSourceConnector",
    "key.converter": "org.apache.kafka.connect.storage.StringConverter",
    "value.converter": "org.apache.kafka.connect.json.JsonConverter",
    "transforms": "tojson,expand",
    "transforms.tojson.type": "com.github.cedelsb.kafka.connect.smt.Record2JsonStringConverter$Value",
    "transforms.expand.type": "com.redhat.insights.expandjsonsmt.ExpandJSON$Value",
    "transforms.expand.sourceFields": "toto",
    "errors.tolerance": "all",
    "errors.log.enable": "true",
    "errors.log.include.messages": "true",
    "splunk.s2s.port": "9997",
    "kafka.topic": "main",
    "splunk.s2s.ssl.enable": "true",
    "splunk.s2s.ssl.key.path": "/etc/kafka/server.key",
    "splunk.s2s.ssl.cert.chain.path": "/etc/kafka/server-cert.pem",
    "value.converter.schemas.enable": "false"
  }
}